{
  "name": "garry-blog",
  "browserslist": [
    "last 2 versions",
    "Firefox ESR",
    "> 1%",
    "ie >= 9",
    "iOS >= 8",
    "Android >= 4"
  ],
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "version": "0.0.1",
  "description": "The blog of Garry",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "npm-run-all --parallel dev:**",
    "dev:serve": "nodemon ./dist/server/main.js --watch 'dist/server/**/*.*' ",
    "dev:build": "webpack --watch --config ./config/webpack.node.dev.conf.ts",
    "serve": "pm2 start ./ecosystem.config.js",
    "stop": "pm2 stop ./dist/server/main.js",
    "build": "npm-run-all --parallel build:app build:dll",
    "build:app": "webpack --config ./config/webpack.prod.conf.ts",
    "build:dll": "webpack --config ./config/webpack.dll.conf.ts",
    "build:server": "webpack --config ./config/webpack.node.prod.conf.ts",
    "build:browser": "webpack --config ./config/webpack.browser.prod.conf.ts",
    "docker:run": "docker run -d -p 80:4000 ssr-service",
    "tslint": "tslint --fix -c ./tslint.json 'src/**/*{.ts,.tsx}'"
  },
  "keywords": [
    "blog",
    "react",
    "webpack4",
    "koa",
    "node"
  ],
  "author": "garry",
  "license": "ISC",
